{
  "name": "telluria-utils-net",
  "description": "Utilities for working with .NET languages",
  "version": "1.8.15",
  "license": "MIT",
  "author": "Telluria Tecnologia",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/t3105/telluria-utils-.net.git"
  },
